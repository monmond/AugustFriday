//MARK: - Imports
import UIKit



//MARK: - Background
public extension UIButton {
  
  public func setBackgroundColor(_ color: UIColor, for state: UIControlState) {
    setBackgroundImage(UIImage.color(with: color), for: state)
  }
  
}
